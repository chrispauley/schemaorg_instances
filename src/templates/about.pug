- var headObject = { title: "schema instances - About", description: "About page.", keywords: "my keywords"}
- var pathObject = { currentPage: "about" }
// myData, openGraphData are coming from gulpfile.js
//
doctype html
html(lang='en')
  include includes/mixins.pug
  //+headSection4_Cloud(myData, openGraphData)
  +headSection4_Cloud(headObject)

  body(id= pathObject.currentPage)
    // Navigation
    +navBar(pathObject)

    // Page Content
    .container
      // Page Heading/Breadcrumbs
      .row
        .col-lg-12
          h1.page-header
            |
            small About this site
          ol.breadcrumb
            li
              a(href='index.html') Home
            li
              a(href='#') About

      // /.row
      .row
        .col-lg-12
          div
            h1= name
            p= author
            p= context
